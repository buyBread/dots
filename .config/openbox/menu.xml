<?xml version="1.0" encoding="utf-8"?>
<openbox_menu xmlns="http://openbox.org/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://openbox.org/                 file:///usr/share/openbox/menu.xsd">
	<menu id="root-menu" label="+ menu">
		<!-- This requires the presence of the 'openbox-menu' package to work -->
		<separator/>
		<item label="Terminal">
			<action name="Execute">
				<execute>urxvt</execute>
			</action>
		</item>
		<item label="Net">
			<action name="Execute">
				<execute>firefox</execute>
			</action>
		</item>
		<item label="Chat">
			<action name="Execute">
				<execute>discord</execute>
			</action>
		</item>
		<item label="Spotify">
			<action name="Execute">
				<execute>spotify</execute>
			</action>
		</item>
		<separator/>
		<menu id="root-menu-35919" label="* Openbox">
			<item label="obconf">
				<action name="Execute">
					<execute>obconf</execute>
				</action>
			</item>
			<item label="Reconfigure">
				<action name="Reconfigure"/>
			</item>
		</menu>
		<separator/>
		<item label="! Logout">
			<action name="Exit"/>
		</item>
		<item label="! Power Off">
			<action name="Execute">
				<execute>systemctl poweroff</execute>
			</action>
		</item>
		<separator/>
	</menu>
</openbox_menu>
