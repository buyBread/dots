<?xml version="1.0" encoding="utf-8"?>
<openbox_menu xmlns="http://openbox.org/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://openbox.org/                 file:///usr/share/openbox/menu.xsd">
	<menu id="root-menu" label="+ menu">
		<!-- This requires the presence of the 'openbox-menu' package to work -->
		<separator/>
		<item label="&gt; term">
			<action name="Execute">
				<execute>urxvt</execute>
			</action>
		</item>
		<item label="&gt; net">
			<action name="Execute">
				<execute>firefox</execute>
			</action>
		</item>
		<item label="&gt; chat">
			<action name="Execute">
				<execute>discord</execute>
			</action>
		</item>
		<item label="&gt; vidya">
			<action name="Execute">
				<execute>steam</execute>
			</action>
		</item>
		<item label="&gt; music">
			<action name="Execute">
				<execute>spotify</execute>
			</action>
		</item>
		<separator/>
		<menu id="root-menu-35919" label="* wm">
			<item label="&gt; obconf">
				<action name="Execute">
					<execute>obconf</execute>
				</action>
			</item>
			<item label="&gt; reconf">
				<action name="Reconfigure"/>
			</item>
		</menu>
		<separator/>
		<item label="&gt; logout">
			<action name="Exit"/>
		</item>
		<item label="&gt; poweroff">
			<action name="Execute">
				<execute>systemctl poweroff</execute>
			</action>
		</item>
		<separator/>
	</menu>
</openbox_menu>
